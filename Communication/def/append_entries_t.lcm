package LcmRaftMessages;

struct append_entries_t
{

    // Protocol specific data
    int64_t timeStamp;
    string sender;
    string receiver;
    int64_t term;
    int64_t nodes;
    string nodeID[10];
    int64_t entryType;
    int64_t leaderCommit;
    int64_t logIndex;
    int64_t prevLogIndex;
    int64_t prevLogTerm;

    // data
    double data[10];
}
